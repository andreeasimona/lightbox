{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
		"jsx-a11y/click-events-have-key-events": 0
    },
    "globals": {
        "document": true,
        "window": true,
        "fetch": true
    }
}
